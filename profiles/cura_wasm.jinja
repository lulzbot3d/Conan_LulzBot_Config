include(cura.jinja)

[tool_requires]
emsdk/3.1.65@lulzbot/stable
nodejs/20.16.0@lulzbot/stable

[settings]
os=Emscripten
arch=wasm

[conf]
tools.build:skip_test=True

[options]
curaenginele/*:enable_plugins=False
curaenginele/*:enable_arcus=False
curator/*:with_cura_resources=True
curator/*:disable_logging=True
libzip/*:crypto=False
